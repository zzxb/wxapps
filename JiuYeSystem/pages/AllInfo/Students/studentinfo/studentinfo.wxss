page{
    background: url(../../../../img/con_bg3.png);
}
.nav{
    display: flex;
    flex-direction: row;
    border-bottom: 2px solid gainsboro;
    width:100%;
    height:100rpx;
}
.topnav{
    line-height:100rpx;
    text-align:center;
    color:#fc5558;
    flex:1;
    border-right:1px solid gainsboro;
    font-weight:bold;
    font-size:28rpx;
}
.default{
    line-height:100rpx;
    text-align:center;flex:1;
    border-right:1px solid gainsboro;
    color:#000;
    font-weight:bold;
    font-size:28rpx;
}
.show{
    display:block;
}
.hidden{
    display:none;
}
/*成绩信息开始*/
.gradecon,.emcon,.unemcon{
    width: 80%;
    height: 700rpx;
    border: 1px solid #ececec;
    border-radius: 5px;
    align-items: center;
    position: relative;
    margin-top: 100rpx;
    margin-left: auto;
    margin-right: auto;
}
.row{
    position: relative;
    float: left;
    margin-left: 30rpx;
    width: 90%;
    margin-top: 50rpx;
   
}
.rowl{
    float: left;
    width: 300rpx;

}
.rowr{
    float: left;
}
/*成绩信息结束*/



/*能力认定开始*/
.starbox{
    width: 90%;
    height:150rpx;
    margin: 0 auto;
    margin-top: 20px;
    align-items: center;
}
.astar{
    background: url(../../../../img/astar.png);
    width: 150px;
    height: 32px;
    position: absolute;
}
.bstar{
    background: url(../../../../img/bstar.png);
    
    height: 32px;
    position: relative;
}
.startext{
    margin: 80rpx auto;
    border: 1px solid #ececec;
    border-radius: 5px;
    padding: 10px;
}
/*能力认定结束*/


