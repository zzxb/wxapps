page{
    background: url(../../../../img/con_bg3.png);
}
.search-box{
    height: 30px;
    margin: 10px;
    border: 1px solid #c6c6c6;
    border-radius: 4px;
    padding: 0;
    position: relative;
    background-color:gainsboro;
}
.show{
    display: block;
}
.hidden{
    display: none;
}
.show,.selectbox{
    font-size: 14px;
    color: #666;
    position: absolute;
    top: 0;
    left: 0;
    height: 30px;
    line-height: 30px;
    text-align: center;
    width: 60px;
}
.show{
    height: 64px;
    line-height: 32px;
    margin: -1px 0 0 -1px;
    background-color: #fff;
    border: 1px solid #c6c6c6; 
    z-index: 20;
    border-radius: 4px 0 4px 4px;
}
.selectbox{
    z-index: 10;
    border-right: 1px solid #c6c6c6;
}
.input{
    margin-left: 61px;
    margin-right: 30px;
    padding: 0 10px;
}
.input input{
    line-height: 30px;
    font-size: 14px;
    height: 30px;
}
.btn-search{
    position: absolute;
    right: 0;
    top: 5px;
    height: 30px;
    width: 25px;
}
.search-box button{
    position: absolute;
    right: 0;
    top: 0;
    height: 30px;
    width: 30px;
    opacity: 0;
}
.studentshow{
    margin: 10px;
    font-size: 14px;
    font-weight: normal;
}
.row1{
    background: #fff;
}
.row2{
    background: #ececec;
    border-radius: 5px;
}
.row1,.row2{
    height: 40px;
    line-height: 40px;
}
.row11{
    float: left;
    width: 120rpx;
    text-align: center;
}
.row22{
    float: left;
    width:230rpx;
    text-align: center;
    height: 40px;
}
.row22 .a{
    font-size: 12px; 
    height: 30px; 
    margin: 4px auto;
   
}
.row33{
    float: left;
    width: 320rpx;
    text-align: center;
}