@import  "../../../app.wxss";
page{
    font-size: 14px;
}
.positionBar{
    width: 100%;
    float: left;
    height: 1.3rem;
    margin: 0px 0px 20px 0px;
    
}
.positionBar image{
     width: 13px;
     height:15px;
     margin: 0px 5px;
}
.loglist{
     width: 90%;
     border: 3px solid #ececec;
     border-radius: 10px;
     padding: 10px;
     margin-top: 20px;
}
.rowheader,.row1,.row2{
    width: 100%;
    height: 30px;
    line-height: 30px;
    display: flex;
    flex-direction: row;
}
.row1{
    background-color: #ececec;
    border-radius: 5px;
}

.lt{
    float: left;
    width: 50%;
    text-align: center;
    font-size: 1.2em;
    font-weight: 600;
}
.ld{
    flex-grow: 1;
    float: left;
    padding-left: 30px;

}
.top{
    display: flex;
    height: 50px;
    line-height: 50px;
    font-size: 22px;
    text-align: center;
    flex-direction: row;
}
.t_item{
    flex-grow: 1;
    background-color: #000;
    border-right: 1px #666 solid;
    border-left: 1px #666 solid;
    color: #fff;
}
.weekrow{
    display: flex;
    flex-direction: row;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #000;
    border-bottom: 1px #000 solid;
}
.week{
    flex-grow: 1;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #000;
}
.datebox{
    background-color: #ececec;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}
.daybox{
    width: 14%;
    flex-grow: 1;
    margin: 1px;
    height: 60px;
    background-color: #fff;
}

















